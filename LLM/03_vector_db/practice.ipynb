    "import pandas as pd\n",
    "import torch\n",
    "from transformers import BertTokenizer, BertForSequenceClassification\n",
    "from torch.utils.data import DataLoader, Dataset\n",
    "\n",
    "# 1. ë°ì´í„° ë¡œë“œ\n",
    "df = pd.read_csv('./data/booking_reviews copy.csv')\n",
    "\n",
